    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/nav_consultations">

    <fragment
        android:id="@+id/nav_consultations"
        android:name="com.example.myapplication.view.ConsultationListFragment"
        android:label="@string/nav_consultations"
        tools:layout="@layout/fragment_consultation_list" />

    <fragment
        android:id="@+id/nav_add_consultation"
        android:name="com.example.myapplication.view.AddConsultationFragment"
        android:label="@string/nav_add_consultation"
        tools:layout="@layout/fragment_add_consultation" />

    <fragment
        android:id="@+id/nav_add_patient"
        android:name="com.example.myapplication.view.AddPatientFragment"
        android:label="@string/nav_add_patient"
        tools:layout="@layout/fragment_add_patient" />

    <fragment
        android:id="@+id/nav_search"
        android:name="com.example.myapplication.view.SearchConsultationsFragment"
        android:label="@string/nav_search"
